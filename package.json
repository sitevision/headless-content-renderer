{
  "name": "@sitevision/headless-content-renderer",
  "version": "0.0.4",
  "author": "Sitevision AB",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "github",
    "url": "https://github.com/sitevision/headless-content-renderer.git"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./cjs/index.js"
    }
  },
  "scripts": {
    "build": "yarn clean && yarn build:esm && yarn build:cjs && cp package.json README.md ./dist",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc",
    "clean": "rimraf dist",
    "test": "cypress run --component",
    "test:watch": "cypress open --component"
  },
  "dependencies": {
    "@types/react": "^18.0.25",
    "react": "^18.3.1",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@testing-library/cypress": "^10.1.0",
    "@types/mocha": "^10.0.4",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "cypress": "^15.3.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "ts-loader": "^9.5.4",
    "webpack": "^5.101.3",
    "webpack-node-externals": "^3.0.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "packageManager": "yarn@4.10.3"
}
